!procedure $rem($name)
    !$handle = '$' + $name
    !$stereotype = '<<' + $name + '>>'
    %set_variable_value($name + '_hidden', 1)
    remove $name
    remove $handle
    remove $stereotype
!endprocedure

!function $ishidden($name)
    !return %get_variable_value($name + '_hidden')
!endfunction

!procedure $hide($name)
    !$handle = '$' + $name
    !$stereotype = '<<' + $name + '>>'
    
    %set_variable_value($name + '_hidden', 1)

    'card ['$name_hidden_var' = $ishidden($name_hidden_var)]

    hide $name
    hide $handle
    hide $stereotype
    skinparam ArrowThickness<<$name>> 0
    skinparam ArrowFontSize<<$name>> 0
!endprocedure

!procedure $show($name)
    !$handle = '$' + $name
    !$stereotype = '<<' + $name + '>>'
    show $name
    show $handle
    show $stereotype
!endprocedure

!unquoted function $link_type($name)
