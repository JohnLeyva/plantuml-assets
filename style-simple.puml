!function $link($text, $url)
    !if (%substr($text,0,1) == '/')
        !$text = $GetPart($url,$text)
    !endif
    !$text = $strReplace($text,'+',' ')
    !return "[[" + $url + " " + $text + "]]"
!endfunction
