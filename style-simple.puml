!function $linkObj($json_object)
    !$res = ''
    !if (%get_json_type($json_object) == "object")
        !foreach $key in %get_json_keys($json_object)
            !$keytag = "<" + $key + ">"
            !$res = $res + " " + $link($keytag,$json_object[$key])
        !endfor
    !endif
    !return $res 
!endprocedure
