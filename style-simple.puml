!function $linkRes($str)
    !$separator="|"
    !$count = $CountPart($str,$separator) -  1
    !$res = ''
    !while ($count >= 0)
        !$part = $GetPart($str,$separator,$count)
        !$key = $GetPart($part,'!',0)
        !$url = $GetPart($part,'!',1)
        !$keytag = "<" + $key + ">"
        !$res = $link($keytag, $url) + " " + $res
        !$count = $count - 1
    !endwhile 
    !return $res 
!endprocedure
